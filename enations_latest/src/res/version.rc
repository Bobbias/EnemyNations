			VALUE "InternalName",    "SChance\0"
			VALUE "LegalCopyright",  "Copyright \251 1995 - 1997 - by Windward Studios\0"
			VALUE "LegalTrademarks", " \0"
			VALUE "OriginalFilename","SChance.exe\0"
			VALUE "ProductName",     "Second Chance\0"
			VALUE "ProductVersion",  "3.1.0.0"
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", RES_LANG, RES_CODEPAGE
//		VALUE "Translation", 0x409, 1252
			// English language (0x409) and the Windows ANSI codepage (1252)
	END
END

